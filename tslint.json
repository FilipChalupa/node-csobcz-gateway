{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-standard-plus"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "interface-name": [true, "never-prefix"],
    "max-line-length": [false, 120],
    "arrow-parens": false,
    "space-before-function-paren": false,
    "no-consecutive-blank-lines": true
  },
  "rulesDirectory": [],
  "linterOptions":{
    "exclude": [
    ]
  }
}
